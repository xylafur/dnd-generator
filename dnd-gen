#!/usr/bin/env python3
from os import sys

from util.parser_util import parser_util, get_parser
from interactive.dnd_gen_shell import run_interactive


if __name__ == '__main__':
    if len(sys.argv) == 1:
        run_interactive()
        exit(0)
    
    parser = get_parser()
    args, leftovers = parser.parse_known_args()

    if len(sys.argv) <= 1:
        parser.print_help()
        parser.exit(1)

    try:
        parser_util(args.which, args)

    except AttributeError:
        print("Error in parsing arguments")
        parser.print_help()
        sys.exit(1)



